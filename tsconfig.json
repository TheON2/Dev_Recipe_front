{
  "compilerOptions": {
    "target": "es2020",
    "noEmit": false,
    "jsx": "react-jsx",
    "module": "es2020",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false,
    "strict": false,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "noImplicitAny": false,
    "noEmitOnError": false
  },
  "include": ["src"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
